{
  "expo": {
    "name": "lyrne-frontend",
    "slug": "lyrne-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lyrne-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "owner": "lyrne",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.lyrne.frontend",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ],
      "expo-router",
      "expo-apple-authentication",
      "@react-native-google-signin/google-signin",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/lyrne-splash-icon.png",
          "backgroundColor": "#232323",
          "dark": {
            "backgroundColor": "#141414"
          },
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fcb48252-9618-40d8-b652-11c446156a4f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b5fa9171-e022-46f2-8343-c42d5dd73199"
    },
    "owner": "lyrne"
  }
}
